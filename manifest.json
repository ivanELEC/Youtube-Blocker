{
	"name": "YouTube Blocker",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
	  "service_worker": "js/background.js"
	},
	"permissions":["storage"],
	"action": {
		"default_popup":"main_ui.html"
	}, 
	"content_scripts":[{
		"matches":["https://youtube.com/*"],
		"js":["js/block_accounts.js"]
	}]
  }